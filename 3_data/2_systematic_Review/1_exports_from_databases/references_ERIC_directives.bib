	note = {ERIC Number: EJ1099364},
	keywords = {Alignment (Education), Educational Practices, Evidence Based Practice, Foreign Countries, Open Education, Outreach Programs, Research Universities, Resource Units, Semi Structured Interviews, Shared Resources and Services, Student Centered Learning, Student Responsibility, Teacher Student Relationship, Undergraduate Study},
	pages = {31--43},
}

